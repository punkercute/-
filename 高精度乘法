#include<stdio.h>
#include <string.h>
int main(void){
    char numberN[1500],numberM[1500];
    scanf("%s %s",numberN,numberM);
    int n=strlen(numberN),m=strlen(numberM);
	int a[n],b[m];
	int i,j,sign; 
	int c[3000]={0};
	int dot1=0,dot2=0;                                                                                                                     
	for(i=0,j=n-1;i<n;i++,j--)
	{
		a[i]=numberN[j]-'0';
	}
	for(i=0,j=m-1;i<m;i++,j--)
	{
		b[i]=numberM[j]-'0';
	}
    for(i=0;i<n;i++)
    {
    	if(a[i]==-2)
    	{
	dot1=n-i;
	for(j=i;j<n;j++)
	{
		a[j]=a[j+1];	
	}a[n-1]=0;	
	}
     }
	for(i=0;i<m;i++)
    {
    	if(b[i]==-2)
    	{
	dot2=m-i;
	for(j=i;j<m;j++)
	{
		b[j]=b[j+1];	
	}b[m-1]=0;	
	}
     }
for (i = 0; i < n; i++)
    {
        for (j = 0; j < m; j++) 
	{
            c[i + j] += a[i] * b[j];
	    sign=i+j+1;
        }
    } 
for (i = 0; i < n + m; i++) 
    {
        if (c[i] >= 10) 
		{
            c[i + 1] += c[i] / 10;
            c[i] %= 10;
        }
    }
    for (i =sign; i >= 0; i--)
    {
	printf("%d", c[i]);
	if(i==m+n-(dot1+dot2))
    	printf(".");	 
    }
    printf("\n");
    return 0;
}
